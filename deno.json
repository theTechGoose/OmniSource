{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "types": ["./globals.d.ts"],
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"]
  },
  "tasks": {},
  "lint": {
    "rules": {
      "exclude": [
        "verbatim-module-syntax",
        "no-prototype-builtins",
        "ban-types",
        "import-map-remap"
      ]
    }
  },
  "imports": {
    "#axios": "npm:axios",
    "#emit": "jsr:@deno/emit",
    "#esbuild": "npm:esbuild@0.20.2",
    "#glob": "npm:glob",
    "#nanoid": "npm:nanoid@5.0.7",
    "#oak": "jsr:@oak/oak@16.1.0",
    "#reflect": "npm:reflect-metadata@0.2.1",
    "#std": "jsr:@libs/std@^2024.10.5",
    "#std/assert": "jsr:@std/assert@1",
    "#zod": "npm:zod",
    "@std/assert": "jsr:@std/assert@1",
    "$std/mock": "jsr:@std/testing/mock",
    "@globals/magic-values": "./magic-values.ts",
    "@raptorize/di": "./libs/raptorize-di/mod.ts",
    "@libs/utilities": "./libs/utilities/mod.ts",
    "@core": "./libs/utilities/core/mod.ts",
    "@higher-order-functions": "./libs/utilities/higher-order-functions/mod.ts",
    "@arrays": "./libs/utilities/arrays/mod.ts",
    "@validation": "./libs/utilities/validation/mod.ts",
    "@string-manipulation": "./libs/utilities/string-manipulation/mod.ts",
    "@core-ui/": "./libs/core-ui/",
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "preact": "https://esm.sh/preact@10.22.0",
    "preact/": "https://esm.sh/preact@10.22.0/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.3.1",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "fresh-stories/": "https://deno.land/x/fresh_stories@0.0.5/",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "tailwindcss/": "npm:/tailwindcss@3.4.1/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.1/plugin.js"
  },
  "workspace": [
    "libs/utilities",
    "libs/raptorize-di",
    "libs/denomite",
    "libs/core-ui"
  ],
  "test": {
    "exclude": ["transfer-*/**/*"]
  }
}
